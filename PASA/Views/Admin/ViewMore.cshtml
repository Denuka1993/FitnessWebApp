@using PASA.Domain
@model Student
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "View More";
}
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="header">
                <h4 class="title">More Detail (@Model.FullName)</h4>
            </div>
            <div class="content">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Full Name</label>:&nbsp;&nbsp;&nbsp;
                            @Model.FullName
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Activity Group</label>:&nbsp;&nbsp;&nbsp;
                            @Model.ActivityGroup
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Date od Birth</label>:&nbsp;&nbsp;&nbsp;
                            @Model.DOB.ToShortDateString()
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Private Address</label>:&nbsp;&nbsp;&nbsp;
                            @Model.AddressPrivate
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>School Address</label>:&nbsp;&nbsp;&nbsp;
                            @Model.AddressSchool
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Telephone No(Residence)</label>:&nbsp;&nbsp;&nbsp;
                            @Model.TeleRes
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Telephone No(Mobile)</label>:&nbsp;&nbsp;&nbsp;
                            @Model.TeleMob
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Email</label>:&nbsp;&nbsp;&nbsp;
                            @Model.Email
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="box-footer">
            @if (Request.UrlReferrer != null)
            {
                <a onclick="history.go(-1); return false;" href="#" class="btn btn-default">Back</a>
            }
        </div>
    </div>
</div>

